{
  "app_id": 131722,
  "title": "CliqueSaúde - Agendamentos",
  "slug": "cliquesaude-agendamentos",
  "type": "external",
  "state": "active",
  "authentication": true,
  "modules": {
    "calculate_shipping": {
      "enabled": true,
      "endpoint": "https://us-central1-cliquesaude-ecomplus.cloudfunctions.net/app/ecom/modules/calculate-shipping"
    }
  },
  "auth_scope": {
    "stores/me": [
      "GET"
    ],
    "procedures": [
      "POST"
    ],
    "products": [],
    "brands": [],
    "categories": [],
    "customers": [
      "GET"
    ],
    "orders": [
      "GET"
    ],
    "carts": [],
    "orders/fulfillments": [],
    "orders/payments_history": [],
    "products/quantity": [],
    "products/variations/quantity": [],
    "products/price": [],
    "products/variations/price": []
  },
  "admin_settings": {
    "zip": {
      "schema": {
        "type": "string",
        "maxLength": 9,
        "pattern": "^[0-9]{5}-?[0-9]{3}$",
        "title": "CEP de origem",
        "description": "Código postal do remetente para cálculo do frete"
      },
      "hide": false
    },
    "services": {
      "schema": {
        "title": "Modalidades de envio com agendamento",
        "type": "array",
        "maxItems": 99,
        "items": {
          "title": "Opção de serviço de entrega",
          "type": "object",
          "required": [
            "service_code",
            "working_open",
            "working_close"
          ],
          "properties": {
            "label": {
              "type": "string",
              "maxLength": 50,
              "title": "Rótulo",
              "description": "Nome do serviço/modalidade exibido aos clientes"
            },
            "service_code": {
              "type": "string",
              "maxLength": 3,
              "pattern": "^[A-Za-z0-9-_.]+$",
              "title": "Código do Serviço"
            },
            "working_open": {
              "type": "string",
              "maxLength": 3,
              "pattern": "^[A-Za-z0-9-_.]+$",
              "title": "Horário de Abertura dos Agendamentos",
              "description": "Exemplo: 7:30"
            },
            "working_close": {
              "type": "string",
              "maxLength": 3,
              "pattern": "^[A-Za-z0-9-_.]+$",
              "title": "Horário de Encerramento dos Agendamentos",
              "description": "Exemplo: 18:00"
            },
            "interval": {
              "type": "number",
              "minimum": 0,
              "maximum": 99999,
              "title": "Intervalo entre agendamentos",
              "description": "Informar intervalo em minutos entre os agendamentos"
            }
          }
        }
      },
      "hide": true
    },
    "posting_deadline": {
      "schema": {
        "title": "Prazo de postagem",
        "type": "object",
        "required": [
          "days"
        ],
        "additionalProperties": false,
        "properties": {
          "days": {
            "type": "integer",
            "minimum": 0,
            "maximum": 999999,
            "title": "Número de dias",
            "description": "Dias de prazo para postar os produtos após a compra"
          },
          "working_days": {
            "type": "boolean",
            "default": true,
            "title": "Dias úteis"
          },
          "after_approval": {
            "type": "boolean",
            "default": true,
            "title": "Após aprovação do pagamento"
          }
        }
      },
      "hide": false
    },
    "additional_price": {
      "schema": {
        "type": "number",
        "minimum": -999999,
        "maximum": 999999,
        "title": "Custo adicional",
        "description": "Valor a adicionar (negativo para descontar) no frete calculado via Jadlog"
      },
      "hide": false
    },
    "shipping_rules": {
      "schema": {
        "title": "Regras de envio/agendamento/coleta",
        "description": "Aplicar descontos/adicionais condicionados",
        "type": "array",
        "maxItems": 300,
        "items": {
          "title": "Regra de envio",
          "type": "object",
          "minProperties": 1,
          "properties": {
            "service_code": {
              "type": "string",
              "maxLength": 3,
              "pattern": "^[A-Za-z0-9-_.]+$",
              "title": "Código do Serviço"
            },
            "zip_range": {
              "title": "Faixa de CEP",
              "type": "object",
              "required": [
                "min",
                "max"
              ],
              "properties": {
                "min": {
                  "type": "integer",
                  "minimum": 0,
                  "maximum": 999999999,
                  "title": "CEP inicial"
                },
                "max": {
                  "type": "integer",
                  "minimum": 0,
                  "maximum": 999999999,
                  "title": "CEP final"
                }
              }
            },
            "min_amount": {
              "type": "number",
              "minimum": 1,
              "maximum": 999999999,
              "title": "Valor mínimo da compra"
            },
            "free_shipping": {
              "type": "boolean",
              "default": false,
              "title": "Frete grátis"
            },
            "discount": {
              "title": "Desconto",
              "type": "object",
              "required": [
                "value"
              ],
              "properties": {
                "percentage": {
                  "type": "boolean",
                  "default": false,
                  "title": "Desconto percentual"
                },
                "value": {
                  "type": "number",
                  "minimum": -99999999,
                  "maximum": 99999999,
                  "title": "Valor do desconto",
                  "description": "Valor percentual/fixo do desconto ou acréscimo (negativo)"
                }
              }
            }
          }
        }
      },
      "hide": false
    }
  },
  "auth_callback_uri": "https://us-central1-cliquesaude-ecomplus.cloudfunctions.net/app/ecom/auth-callback",
  "version": "1.0.0"
}